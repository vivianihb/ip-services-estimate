CREATE DATABASE [ORCAMENTOS]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'ORCAMENTOS', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL15.MSSQLSERVER\MSSQL\DATA\ORCAMENTOS.mdf' , SIZE = 8192KB , MAXSIZE = UNLIMITED, FILEGROWTH = 65536KB )
 LOG ON 
( NAME = N'ORCAMENTOS_log', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL15.MSSQLSERVER\MSSQL\DATA\ORCAMENTOS_log.ldf' , SIZE = 8192KB , MAXSIZE = 2048GB , FILEGROWTH = 65536KB )
 WITH CATALOG_COLLATION = DATABASE_DEFAULT
GO
ALTER DATABASE [ORCAMENTOS] SET COMPATIBILITY_LEVEL = 150
GO
IF (1 = FULLTEXTSERVICEPROPERTY('IsFullTextInstalled'))
begin
EXEC [ORCAMENTOS].[dbo].[sp_fulltext_database] @action = 'enable'
end
GO
ALTER DATABASE [ORCAMENTOS] SET ANSI_NULL_DEFAULT OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET ANSI_NULLS OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET ANSI_PADDING OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET ANSI_WARNINGS OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET ARITHABORT OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET AUTO_CLOSE OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET AUTO_SHRINK OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET AUTO_UPDATE_STATISTICS ON 
GO
ALTER DATABASE [ORCAMENTOS] SET CURSOR_CLOSE_ON_COMMIT OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET CURSOR_DEFAULT  GLOBAL 
GO
ALTER DATABASE [ORCAMENTOS] SET CONCAT_NULL_YIELDS_NULL OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET NUMERIC_ROUNDABORT OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET QUOTED_IDENTIFIER OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET RECURSIVE_TRIGGERS OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET  ENABLE_BROKER 
GO
ALTER DATABASE [ORCAMENTOS] SET AUTO_UPDATE_STATISTICS_ASYNC OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET DATE_CORRELATION_OPTIMIZATION OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET TRUSTWORTHY OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET ALLOW_SNAPSHOT_ISOLATION OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET PARAMETERIZATION SIMPLE 
GO
ALTER DATABASE [ORCAMENTOS] SET READ_COMMITTED_SNAPSHOT OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET HONOR_BROKER_PRIORITY OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET RECOVERY FULL 
GO
ALTER DATABASE [ORCAMENTOS] SET  MULTI_USER 
GO
ALTER DATABASE [ORCAMENTOS] SET PAGE_VERIFY CHECKSUM  
GO
ALTER DATABASE [ORCAMENTOS] SET DB_CHAINING OFF 
GO
ALTER DATABASE [ORCAMENTOS] SET FILESTREAM( NON_TRANSACTED_ACCESS = OFF ) 
GO
ALTER DATABASE [ORCAMENTOS] SET TARGET_RECOVERY_TIME = 60 SECONDS 
GO
ALTER DATABASE [ORCAMENTOS] SET DELAYED_DURABILITY = DISABLED 
GO
ALTER DATABASE [ORCAMENTOS] SET ACCELERATED_DATABASE_RECOVERY = OFF  
GO
EXEC sys.sp_db_vardecimal_storage_format N'ORCAMENTOS', N'ON'
GO
ALTER DATABASE [ORCAMENTOS] SET QUERY_STORE = OFF
GO
USE [ORCAMENTOS]
GO
